{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
        "@/*": ["app/*"],
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/login.js",
    "app/(public)/login/page.js*.ts",
    "app/(private)/layout.js",
    "app/login/page.js",
    "utils/axios.js",
    "app/page.jsx",
    "app/layout.jsx",
    "context/AuthProvider.ss",
    ".next/types/**/*.ts"
, "app/auth/getUser/route.js"  ],
  "exclude": [
    "node_modules"
  ]
}
